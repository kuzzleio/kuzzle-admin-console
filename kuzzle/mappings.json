
{
  "kuzzle-bo-testindex": {
    "kuzzle-bo-complex-mapping": {
      "dynamic": "strict",
      "properties": {
        "binary": {
          "type": "binary"
        },
        "boolean": {
          "type": "boolean"
        },
        "date": {
          "type": "date",
          "format": "YYYY-MM-dd HH:mm:ss"
        },
        "geo_point": {
          "type": "geo_point"
        },
        "geo_point_lat_lon": {
          "type": "geo_point",
          "lat_lon": true
        },
        "geo_shape": {
          "type": "geo_shape"
        },
        "ip": {
          "type": "ip"
        },
        "nested": {
          "type": "nested",
          "dynamic": false,
          "properties": {
            "sub_string": {
              "type": "string"
            },
            "sub_boolean": {
              "type": "boolean"
            }
          }
        },
        "integer": {
          "type": "integer"
        },
        "array_of_integer": {
          "type": "integer"
        },
        "float": {
          "type": "float"
        },
        "object": {
          "type": "object",
          "dynamic": true,
          "properties": {
            "sub_geo_point": {
              "type": "geo_point"
            }
          }
        },
        "string": {
          "type": "string"
        },
        "array_of_string": {
          "type": "string"
        }
      }
    }
  }
}