{
  "name": "kuzzle-backoffice",
  "version": "2.1.2",
  "description": "A handy back-office for Kuzzle.io",
  "author": "The Kuzzle team <support@kuzzle.io>",
  "private": false,
  "bin": "./bin/kuzzle-backoffice",
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "codecov": "cat test/unit/coverage/lcov.info | ./node_modules/.bin/codecov",
    "e2e-chrome": "docker-compose -f test/e2e/docker-compose.yml -p e2e-chrome run testim",
    "e2e-firefox": "BROWSER=firefox docker-compose -f test/e2e/docker-compose.yml -p e2e-firefox run testim",
    "e2e": "npm run e2e-chrome && npm run e2e-firefox",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "install_deps": "npm i && npm i -g bower && bower install --allow-root --no-interactive",
    "prepublishOnly": "npm run build",
    "start": "http-server dist"
  },
  "keywords": [
    "kuzzle",
    "backoffice"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kuzzleio/kuzzle-backoffice.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "commander": "^2.9.0",
    "finalhandler": "^1.0.1",
    "http-server": "0.9.0",
    "lodash": "^4.17.2",
    "serve-static": "^1.12.1",
    "vue-quill-editor": "^1.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
